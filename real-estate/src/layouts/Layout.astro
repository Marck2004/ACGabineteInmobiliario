---
import GoogleSvg from "@/assets/svg/Google-svg.astro";
import WhatsappSvg from "@/assets/svg/Whatsapp-svg.astro";
import Footer from "@/components/Footer.astro";
import Navbar from "@/components/Navbar.astro";
import "@/styles/global.css";
import { ClientRouter } from "astro:transitions";
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/logo.svg" />
    <meta name="generator" content={Astro.generator} />
    <title>AC Gabinete inmobiliario</title>
    <ClientRouter />
  </head>
  <body>
    <Navbar />
    <slot />
    <Footer />
    <div class="flex fixed right-15 bottom-15 z-50 gap-5">
      <GoogleSvg />
      <WhatsappSvg />
    </div>
  </body>
</html>
